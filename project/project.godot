; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Resource",
"class": "FestivalR",
"language": "GDScript",
"path": "res://scripts/FestivalR.gd"
}, {
"base": "Resource",
"class": "SaveGame",
"language": "GDScript",
"path": "res://scripts/SaveGameR.gd"
}, {
"base": "Resource",
"class": "Skill",
"language": "GDScript",
"path": "res://skills/RSkill.gd"
}, {
"base": "Resource",
"class": "SkillSet",
"language": "GDScript",
"path": "res://skills/RSkillSet.gd"
} ]
_global_script_class_icons={
"FestivalR": "",
"SaveGame": "",
"Skill": "",
"SkillSet": ""
}

[application]

config/name="InnKeeperUnlimited"
run/main_scene="res://Inn.tscn"
config/icon="res://textures/ale_icon.png"

[autoload]

S="*res://scripts/Stats.gd"

[display]

window/size/width=480
window/size/height=720
window/handheld/orientation="portrait"
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[gui]

common/drop_mouse_on_gui_input_disabled=true
theme/custom="res://resources/new_theme.tres"
theme/custom_font="res://resources/roxato.tres"

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/normal_map_invert_y": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[locale]

translations=PoolStringArray( "res://resources/rest_translations.en.translation", "res://resources/ale_translations.en.translation", "res://resources/potion_translations.en.translation", "res://resources/office_translations.en.translation" )
locale_filter=[ 0, [  ] ]

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
2d/snapping/use_gpu_pixel_snap=true
vram_compression/import_etc=true
environment/default_environment="res://default_env.tres"
